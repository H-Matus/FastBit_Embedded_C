#include <stdint.h>
#include <stdio.h>

#define NUM_OF_NUMS 3

double average(float num1, float num2, float num3);

int main(void)
{
	double arr[NUM_OF_NUMS];

	for(int i = 0; i < NUM_OF_NUMS; i++)
	{
		scanf("%f", &arr[i]);
	}

	return 0;
}

double average(float num1, float num2, float num3)
{
	return (num1 + num2 + num3)/3;
}
