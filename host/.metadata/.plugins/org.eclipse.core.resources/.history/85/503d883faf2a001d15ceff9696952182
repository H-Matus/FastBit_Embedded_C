#include <stdio.h>
#include <stdint.h>



int main(void)
{
	char val = 100;
	printf("Address of the val = %s\n", &val);

	char* ptrVal = &val;

	char value = *ptrVal;

	printf("Read operation yield from ptrVal = %d\n", *ptrVal);
	*ptrVal = 65;

	printf("Address of the val = %s\n", &val);

	getchar();
	getchar();

}
